   <SourceLine>http.SetRequestHeader("X-EBAY-SOA-SECURITY-TOKEN",me.userToken)</SourceLine>
   <SourceLine>http.SetRequestHeader("X-EBAY-SOA-OPERATION-NAME","getJobStatus")</SourceLine>
