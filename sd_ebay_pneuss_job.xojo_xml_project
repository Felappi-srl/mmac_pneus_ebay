   <SourceLine>case "CH"</SourceLine>
   <SourceLine>cEbay.userToken=IT_Token</SourceLine>
   <SourceLine>cEbay.siteID="193"</SourceLine>
   <SourceLine>sFile="ebay_revise_ch.csv"</SourceLine>
   <SourceLine>else</SourceLine>
