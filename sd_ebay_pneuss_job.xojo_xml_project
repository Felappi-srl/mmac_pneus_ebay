   <SourceLine>if  instr(campi(0),"_")&gt;0  then</SourceLine>
   <SourceLine>mpn =mid( campi(0),instr(campi(0),"_") + 1)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>mpn = campi(0)</SourceLine>
   <SourceLine>end</SourceLine>
