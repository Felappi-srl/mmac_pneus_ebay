   <SourceLine>specifiche = Split(campi(27),"§")</SourceLine>
   <SourceLine>valspecifiche = Split(campi(28),"§")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>outputFile = outputFile+cr+ "&lt;ItemSpecifics&gt;"</SourceLine>
