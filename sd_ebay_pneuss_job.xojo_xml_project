   <SourceLine></SourceLine>
   <SourceLine>me.zar = archive</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>me.markStart = "snapshot-begin-"</SourceLine>
   <SourceLine>me.markEnd = "snapshot-end-"</SourceLine>
