   <SourceLine>else</SourceLine>
   <SourceLine>shippingservice= "IT_ExpressCourier"</SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>shippingservice= "IT_ExpressCourier"</SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if days = "" then </SourceLine>
   <SourceLine>days = "1"</SourceLine>
   <SourceLine>else </SourceLine>
   <SourceLine>days = days</SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if siteid="101" then        '------------ ITALY (IT) ---------------------------------</SourceLine>
   <SourceLine></SourceLine>
