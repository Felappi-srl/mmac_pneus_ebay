   <SourceLine>dim zar as new ZipArchive</SourceLine>
   <SourceLine>ZipArchive.RegisterLicense "Alessandro Conti", 1694156619</SourceLine>
   <SourceLine>zar.RegisterLicense "Alessandro Conti", 1694156619</SourceLine>
